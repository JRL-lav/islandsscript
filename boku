
local LocalPlayer = game:GetService("Players").LocalPlayer
local AntiAutoClick = LocalPlayer.PlayerGui.SaveGui.AntiAutoClick
local TextMatch = {"Red","Orange","Yellow","Green","Blue"}
local VirtualUser = game:GetService("VirtualUser")
AntiAutoClick:GetPropertyChangedSignal("Visible"):Connect(function()
	if AntiAutoClick.Visible == true then
		wait(0.1)
		local Color
		for i,v in pairs(TextMatch) do
			if string.match(AntiAutoClick.TextFrame.ColorIndicator.Text,v) then
				Color = v
			end
		end
		for i,v in pairs(AntiAutoClick.Colors:GetChildren()) do
			if v.Button.Text == Color:sub(1,1) then
				firesignal(v.Button.MouseButton1Click)
			end
		end
	end
end)
LocalPlayer.Idled:connect(function()
	VirtualUser:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	wait(1)
	VirtualUser:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)



if game:GetService("CoreGui"):FindFirstChild("Boku No Roblox: Remastered") then
	game:GetService("CoreGui").Venyx:Destroy()
end



local library = loadstring(game:HttpGet("https://pastebin.com/raw/qwdPKKDN"))()
local venyx = library.new("Venyx", 5013109572)


game:GetService("CoreGui").Venyx.Main.Size = UDim2.new(0,550,0,400)


-- themes
local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),  
	TextColor = Color3.fromRGB(255, 255, 255)
}


-- first page
local page = venyx:addPage("Test", 5012544693)
local section1 = page:addSection("                                           --// Auto Farming //--")
local section2 = page:addSection("                                         --// Farming Settings //--")
local section3 = page:addSection("                                           --// Stat Points //--")

local player = game.Players.LocalPlayer
local path = game:GetService("Workspace").NPCs:GetChildren()
mobs = {}
local qc = "Kill Criminal"
local af = false
local mob = "Criminal"
local Tween = game:GetService'TweenService';
local ab = false
local as = false

for i = 1,#path do local v = path[i]
	if not table.find(mobs, v.Name)then 
		table.insert(mobs, v.Name) 
	end 
end 
local speed = 1


section1:addToggle("AutoFarm",nil,function(bool)
	af = bool
end)

section1:addDropdown("Criminal",mobs,function(a)
	mob = a
	qc = "Kill "..mob
	if mob == "UA Student" or mob == "UA Student 2" or mob == "UA Student 3" or mob == "UA Student 4" or mob == "UA Student 5" then
		qc = "Defeat UA Student"	
	end
	if mob == "Weak Nomu 1" or mob == "Weak Nomu 2" or mob == "Weak Nomu 3" or mob == "Weak Nomu 4" or mob == "Weak Nomu 5" then
		qc = "Kill Weak Nomu"	
	end
	if mob == "Pro Hero 1" or mob == "Pro Hero 2" or mob == "Pro Hero 3" or mob == "Pro Hero 4" or mob == "Pro Hero 5" then
		qc = "Kill Pro Hero"	
	end
	if mob == "High End 1" or mob == "High End 2" or mob == "High End 3" or mob == "High End 4" or mob == "High End 5" then
		qc = "Kill High End"	
	end
	if mob == "All Might 1" then
		qc = "Defeat All Might"
	end
end)
section1:addToggle("Auto Punching Bag(Use if lower than lvl 20)",nil,function(bool)
	ab = bool
end)

section2:addSlider("Tp Speed", 1, 0.1, 1, function(value)
	speed = value
end)


section2:addToggle("Use Sword/Dagger",nil,function(bool)
	as = bool
end)
local ats = false
local atb = false
local ata = false

section3:addToggle("Strength",nil,function(bool)
	ats = bool
	while true do
		if ats then

			local args = {
				[1] = 1
			}

			game:GetService("ReplicatedStorage").Remotes.Strength:FireServer(unpack(args))
		end
		wait(1)
	end
		
	
end)

section3:addToggle("Durability",nil,function(bool)
	atb = bool
	while true do
		if atb then


			local args = {
				[1] = 1
			}

			game:GetService("ReplicatedStorage").Remotes.Durability:FireServer(unpack(args))

		end
		wait(1)
	end


end)
section3:addToggle("Agility",nil,function(bool)
	atb = bool
	while true do
		if ata then

			local args = {
				[1] = 1
			}

			game:GetService("ReplicatedStorage").Remotes.Agility:FireServer(unpack(args))
		end
		wait(1)
	end


end)



_G.Quirk = "Quirk1"
_G.Key = "E"

for k,v in pairs(player.Character:GetChildren()) do 
	if string.find(v.Name, "Quirk") then 
		_G.Quirk = v.Name
	end
end

function farm()
	pcall(function()
		while true do
			if player.Character.Humanoid.Health > 0 then
				for k,v in pairs(workspace.NPCs:GetChildren()) do
					if af and player.Character.Humanoid.Health > 0 and v.Name == mob then
						repeat
							if player.Character.Humanoid.Health > 0 then
								local char = player.Character;
								local NPC = v
								Tween:Create(char.HumanoidRootPart, TweenInfo.new(speed), {CFrame = NPC.HumanoidRootPart.CFrame * CFrame.new(0, 0, 2)}):Play()
								local PunchEvent = game:GetService("Workspace")[player.Name][_G.Quirk][_G.Key]
								PunchEvent:FireServer()
								local PunchEvent2 = game:GetService("Workspace")[player.Name][_G.Quirk].E
								PunchEvent2:FireServer()
								local QuestEvent = game:GetService("ReplicatedStorage").Remotes.Quest.AcceptQuest
								QuestEvent:FireServer(qc)

								local QuestEvent2 = game:GetService("ReplicatedStorage").Remotes.Quest.CompleteQuest
								QuestEvent2:FireServer(qc)
							
								wait(.05)
							else
								wait(2)
							end
						until not af or as or v.Humanoid.Health < 1
					end
					wait()
				end
			else
				wait(2)
			end
			wait()
		end
	end)	
end
function bag()
	while true do
		if ab then
			repeat wait()
				local char = player.Character;

				Tween:Create(char.HumanoidRootPart, TweenInfo.new(0.1), {CFrame = game:GetService("Workspace")["Punching Bags"]["Punching bag"]["Punching bag"].HumanoidRootPart.CFrame * CFrame.new(0, -1, 3)}):Play()
				local PunchEvent = game:GetService("Workspace")[player.Name][_G.Quirk][_G.Key]
				PunchEvent:FireServer()
				local PunchEvent2 = game:GetService("Workspace")[player.Name][_G.Quirk].E
				PunchEvent2:FireServer()
	
			until not ab
		end
		wait()
	end
end

function farm2()
	pcall(function()

		local player = game.Players.LocalPlayer
		_G.Active = true
		_G.Enemy = "Criminal"
		_G.Quirk = "Quirk1"
		_G.Key = "E"

		for k,v in pairs(player.Character:GetChildren()) do 
			if string.find(v.Name, "Quirk") then 
				_G.Quirk = v.Name
			end
		end

		while af do

			if player.Character.Humanoid.Health > 0 then

				for k,v in pairs(workspace.NPCs:GetChildren()) do

					if af and player.Character.Humanoid.Health > 0 and v.Name == mob then
						if not as then return end
						repeat
							if player.Character.Humanoid.Health > 0 then



								local char = player.Character;


								local NPC = v
								Tween:Create(char.HumanoidRootPart, TweenInfo.new(1), {CFrame = NPC.HumanoidRootPart.CFrame * CFrame.new(0, 0, 2)}):Play();




								local args = {
									[1] = CFrame.new(1093.62988, 325.807129, 246.272186, -0.872535467, -0.210645035, 0.440806776, 1.49011612e-08, 0.902274132, 0.431162983, -0.488550872, 0.376204997, -0.787266135)
								}

								game:GetService("Players").LocalPlayer.Character.Main.Swing:FireServer(unpack(args))


								local QuestEvent = game:GetService("ReplicatedStorage").Remotes.Quest.AcceptQuest
								QuestEvent:FireServer(qc)

								local QuestEvent2 = game:GetService("ReplicatedStorage").Remotes.Quest.CompleteQuest
								QuestEvent2:FireServer(qc)

								wait(.05)

							else

								wait(2)

							end

						until not af or not as or v.Humanoid.Health < 1



					end

					wait()

				end

			else

				wait(2)

			end

			wait()

		end
	end)	
end
spawn(function()
	while wait() do
		if af then
			farm()
		end
	end
end)
spawn(function()
	while wait() do
		if ab then
			bag()
		end
	end
end)
spawn(function()
	while wait() do
		if af then
			farm2()
		end
	end
end)



local pages = venyx:addPage("Settings", 5012544693)
local section = pages:addSection("                                           --// Settings //--")
local TeleportService = game:GetService("TeleportService")


section:addKeybind("Toggle Keybind", Enum.KeyCode.P, function()
	print("Activated Keybind")
	venyx:toggle()
end, function()
	print("Changed Keybind")
end)
section:addButton("Rejoin Game",function(a)
	TeleportService:Teleport(game.PlaceId,player)
end)
section:addButton("Reload Script",function(a)
	game:GetService("CoreGui").Venyx:Destroy()






end)

local colors = pages:addSection("Theme")

for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
	colors:addColorPicker(theme, color, function(color3)
		venyx:setTheme(theme, color3)
	end)
end

venyx:Notify("by JRL#8053", "Fuck off")
game:GetService("CoreGui").Venyx.Notification.Size = UDim2.new(0,150,0,60)
venyx:SelectPage(venyx.pages[1], true) -- no default for more freedom

print("Loaded")
print("Thanks For Using My Script "..player.Name)


